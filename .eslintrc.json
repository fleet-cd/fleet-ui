{
    "extends": [
        "next/core-web-vitals",
        "eslint:recommended",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
        "sort-imports": [
            "error",
            {
                "ignoreCase": true,
                "ignoreDeclarationSort": true
            }
        ],
        "quotes": ["error", "double"],
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": ["error"],
        "prefer-template": ["error"]
    }
}